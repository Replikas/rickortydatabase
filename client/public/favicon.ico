  <!-- Portal rings -->
  <circle cx="16" cy="16" r="14" fill="none" stroke="#00ff88" stroke-width="1" opacity="0.3"/>
  <circle cx="16" cy="16" r="11" fill="none" stroke="#00ccff" stroke-width="1" opacity="0.5"/>
  
  <!-- Central portal -->
  <circle cx="16" cy="16" r="8" fill="url(#portalGradient)"/>
  
  <!-- Simple R&M text -->
  <text x="16" y="20" font-family="Arial, sans-serif" font-size="8" font-weight="bold" 
